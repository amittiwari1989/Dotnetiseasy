<!DOCTYPE HTML>
<html>
<head>
    <title>@ViewBag.Title</title>
    <meta charset="utf-8" />
    <link rel="icon" href="~/Image/Icon.ico" type="image/x-icon" sizes="16x16"> 
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="description" content="Learn to develop basic site and basic software, share and download sample projects" />
    <meta name="description" content="@ViewBag.meta" />
    <meta name="keywords" content="@ViewBag.meta">
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
    <link rel="stylesheet" href="~/Content/css/main.css" />
    <link rel="stylesheet" href="~/Content/bootstrap-4.4.1-dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
    
    <script src="~/Scripts/js/jquery.min.js"></script>
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-162099971-1"></script>
    <script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', 'UA-162099971-1');
    </script>

</head>
<body class="is-preload">

    <!-- Wrapper -->
    <div id="wrapper">

        <!-- Main -->
        <div id="main">
            <div class="inner">

                <!-- Header -->
                <header id="header">
                    <a href="~/" class="logo"><img src="~/Image/LogoP.png" style="position: absolute;margin-top: -50px;height:85px" /> </a>
                    
                    <ul class="icons">
                        <li><a href="#" class="icon brands fa-twitter"><span class="label">Twitter</span></a></li>
                        <li><a href="#" class="icon brands fa-facebook-f"><span class="label">Facebook</span></a></li>
                        <li><a href="#" class="icon brands fa-snapchat-ghost"><span class="label">Snapchat</span></a></li>
                        <li><a href="#" class="icon brands fa-instagram"><span class="label">Instagram</span></a></li>
                        <li><a href="#" class="icon brands fa-medium-m"><span class="label">Medium</span></a></li>
                      @if (Session["DisplayName"] != null){  <li style="position: absolute;margin-left: -311px;margin-top: -39px;width: 300px;">Hi @Session["DisplayName"] <a href="~/Account/Logout">Logout</a></li> }
                    </ul>
                    
                </header>

                <!-- Content -->
                <section>
                    @RenderBody()

                </section>

            </div>
        </div>

        <!-- Sidebar -->
        <div id="sidebar">
            <div class="inner">

                <!-- Search -->
                <section id="search" class="alt">

                    @using (Html.BeginForm("Index", "Artical", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
                    {
                        @Html.AntiForgeryToken()
                        <input type="text" name="Artials" id="Artials" placeholder="Search" />
                    }
                </section>

                <!-- Menu -->
                <nav id="menu">
                    <header class="major">
                        <h2>Menu</h2>
                    </header>
                    <ul>
                        <li><a href="~/">Home</a></li>
                        <li><a href="~/DotNet">ASP.NET</a></li>
                        <li><a href="~/SQL">SQL</a></li>
                        <li><a href="~/HTML">HTML</a></li>
                        <li><a href="~/Practice">MCQ</a></li>
@if (Session["DisplayName"] != null)
{
                        <li><a href="~/Artical/PostArtical">Post Artical</a></li>

                        <li>
                            <span class="opener">Account</span>
                            <ul>
                                <li><a href="~/Account/ChangePassword">Change Password</a></li>
                                <li><a href="~/Artical/MyArticals">My Articals</a></li>
                                <li><a href="~/Practice/MyTestHistory">My Test History</a></li>
                                <li><a href="~/Practice/AddNewTest">Add New Test</a></li>
                                <li><a href="~/Practice/MyQuestionSet">My Question Set</a></li>
                            </ul>
                        </li>
}
else
{
                      <li><a href="~/Account/CreateUser">Register</a></li>
                    <li><a href="~/Account/Login">Login</a></li>
}
                    </ul>
                </nav>

                <!-- Section -->
                

                <!-- Section -->
                <section>
                    <header class="major">
                        <h2>Get in touch</h2>
                    </header>
                    <p>Contact Details.</p>
                    <ul class="contact">
                        <li class="icon solid fa-envelope"><a href="mailto:info@dotnetiseasy.com">info@dotnetiseasy.com</a></li>
                        @*<li class="icon solid fa-phone">(000) 000-0000</li>*@
                        <li class="icon solid fa-home">
                            Mumbai<br />
                            40012008
                        </li>
                    </ul>
                </section>

                <!-- Footer -->
                <footer id="footer">
                    <a href="~/Home/PrivacyPolicy">Privacy Policy</a><br />
                    <a href="~/Home/TermsOfService">Terms of Service</a><br />
                    <p class="copyright">copyright &copy; 2020</p>
                </footer>

            </div>
        </div>

    </div>

    <!-- Scripts -->

    <script src="~/Scripts/js/browser.min.js"></script>
    <script src="~/Scripts/js/breakpoints.min.js"></script>
    <script src="~/Scripts/js/main.js"></script>
    <script src="~/Content/bootstrap-4.4.1-dist/js/bootstrap.min.js"></script>

    @*@if (Session["uid"] == null || (string.IsNullOrEmpty(Model)))
    { }
    else
    {*@
    
    @*}*@

    
</body>
</html>