<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!-->
<html class="no-js">
<!--<![endif]-->
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="description" content="@ViewBag.meta" />
    <meta name="keywords" content="@ViewBag.meta">

    <title>@ViewBag.Title</title>
    <!-- Mobile viewport -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=yes">

    <link rel="shortcut icon" href="images/favicon.ico" type="image/x-icon">

    <link href='http://fonts.googleapis.com/css?family=Droid+Serif|Ubuntu' rel='stylesheet' type='text/css'>

    <link rel="stylesheet" href="~/Content/normalize.css">
    <link rel="stylesheet" href="~/Scripts/flexslider/flexslider.css">
    <link rel="stylesheet" href="~/Content/basic-style.css">
    <link rel="stylesheet" href="~/Content/css/main.css" />
    <link href="~/Content/prism.css" type="text/css" rel="stylesheet" />

    <!-- JS-->
    <script src="~/Scripts/libs/modernizr-2.6.2.min.js"></script>
    <!-- end JS-->


    <link href="https://cdnjs.cloudflare.com/ajax/libs/prism/1.19.0/themes/prism-coy.min.css" type="text/css" rel="stylesheet" />
    <script type="text/javascript" src="~/Scripts/prefixfree.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/prism/1.19.0/components/prism-csharp.js"></script>
</head>


<body id="home">
    <!-- header area -->
    <header class="wrapper clearfix">

        <div id="logo" style="float:left">
            <div id="logo"><a href="http://www.dotnetiseasy.com/"><img src="~/Image/logoP.png" alt="dotnetiseasy" style="height: 90px;position: absolute;margin-top: -15px;"></a></div>
        </div>

        <!-- main navigation -->
        <nav id="topnav" role="navigation">
            <div class="menu-toggle">Menu</div>
            <ul class="srt-menu" id="menu-main-navigation">
                <li>
                    @using (Html.BeginForm("Index", "Artical", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
                    {
                        @Html.AntiForgeryToken()
                        <div class="form-group">
                            <div class="col-md-10">
                                @*@Html.TextBoxFor(m => m.search_tags, new { @class = "form-control" })*@
                                <input type="text" name="Artials" class="form-group" />

                            </div>
                        </div>
                    }
                </li>
                <li class="current"><a href="http://www.dotnetiseasy.com/">Home</a></li>
                <li>
                    <a href="#">Source</a>

                    <ul>
                        <li>
                            <a href="~/DotNet">ASP.NET</a>
                        </li>
                        <li>
                            <a href="~/SQL">SQL</a>
                        </li>
                        <li>
                            <a href="~/HTML">HTML</a>
                        </li>
                    </ul>
                </li>
            </ul>
        </nav><!-- end main navigation -->

    </header><!-- end header -->

    <section id="hero" class="clearfix">
        <!-- responsive FlexSlider image slideshow -->
        <div id="wrapper">

            <!-- Main -->
            <div id="main">

                <div class="inner">
                    <section>
                        @RenderBody()
                    </section>
                </div>
            </div>
        </div>
    </section>
    <!-- jQuery -->
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="js/libs/jquery-1.9.0.min.js">\x3C/script>')</script>

    @*<script defer src="js/flexslider/jquery.flexslider-min.js"></script>*@

    <!-- fire ups - read this file!  -->
    <script src="~/Scripts/main.js"></script>
    <script src="~/Scripts/prism.js"></script>
    @*<script src="~/Scripts/prism-line-numbers.js"></script>*@
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/prism/1.19.0/components/prism-csharp.js"></script>



</body>
</html>